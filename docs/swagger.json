  "components": {
    "schemas": {
      "AuthLevel": {
        "enum": [
          7,
          11,
          13,
          17,
          23,
          29,
          41
        ],
        "type": "integer",
        "description": "This is the authorization levels for an employee\r\ngiven the level determines the privileges",
        "format": "int32"
      },
      "securityDefinitions": {
        "api_key": {
          "type": "apiKey",
          "name": "api_key",
          "in": "header"
        },
        "petstore_auth": {
          "type": "oauth2",
          "authorizationUrl": "https://localhost:44316/auth/api/login",
          "flow": "implicit",
          "scopes": {
            "write:pets": "Add API",
          }
        }
      },
      "AuthRegisterDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "username": {
            "type": "string",
            "nullable": true
          },
          "pasword": {
            "type": "string",
            "nullable": true
          },
          "autheticationLevel": {
            "$ref": "#/components/schemas/AuthLevel"
          }
        },
        "additionalProperties": false,
        "description": "Smaller dto as to not expose the hash fields to a user"
      },
      "TaxCalculationItemEvent": {
        "type": "object",
        "properties": {
          "calcId": {
            "type": "string",
            "format": "uuid"
          },
          "amount": {
            "type": "number",
            "format": "double"
          },
          "percent": {
            "type": "number",
            "format": "double"
          },
          "calculatedAmount": {
            "type": "number",
            "format": "double"
          },
          "createdDate": {
            "type": "string",
            "format": "date-time"
          },
          "taxId": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false,
        "description": "This is the data object responsible for transporting and storing the\r\ncalculated results from the api calls"
      },
      "SubRate": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "rate_id": {
            "type": "string",
            "format": "uuid"
          },
          "city": {
            "type": "string",
            "nullable": true
          },
          "city_rate": {
            "type": "string",
            "nullable": true
          },
          "combined_district_rate": {
            "type": "string",
            "nullable": true
          },
          "combined_rate": {
            "type": "string",
            "nullable": true
          },
          "country": {
            "type": "string",
            "nullable": true
          },
          "country_rate": {
            "type": "string",
            "nullable": true
          },
          "county": {
            "type": "string",
            "nullable": true
          },
          "county_rate": {
            "type": "string",
            "nullable": true
          },
          "freight_taxable": {
            "type": "boolean"
          },
          "state": {
            "type": "string",
            "nullable": true
          },
          "state_rate": {
            "type": "string",
            "nullable": true
          },
          "zip": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false,
        "description": "The rate is the main object at this point\r\nthis will carry oll the information needed"
      },
      "RatesRate": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "rate": {
            "$ref": "#/components/schemas/SubRate"
          }
        },
        "additionalProperties": false,
        "description": "Rates is the wrapper for the rate class so that JSON can serialize and deserialize"
      }
    }
  }
